{
	"publisher": "codegroove",
	"name": "codegroove",
	"displayName": "codegroove",
	"description": "codegroove is vscode plugin to track and analyze coding time",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.85.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/k-stopczynska/codegroove"
	},
	"keywords": [
		"code time",
		"code",
		"time",
		"data",
		"wakatime",
		"codetime",
		"worktime"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codegroove.showStats",
				"title": "show stats"
			},
			{
				"command": "codegroove.pickInactivityTime",
				"title": "pick inactivity time"
			},
			{
				"command": "codegroove.playGroove",
				"title": "play some music"
			}
		],
		"configuration": {
			"title": "codegroove",
			"properties": {
				"codegroove.inactivityThreshold": {
					"type": "number",
					"default": 15,
					"enum": [
						5,
						15,
						30,
						45,
						60
					],
					"enumDescriptions": [
						"5mins",
						"15mins",
						"30mins",
						"45mins",
						"1hour"
					],
					"description": "Pick inactivity threshold"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test --timeout 5000"
	},
	"devDependencies": {
		"@types/chai": "^4.3.12",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/uuid": "^9.0.8",
		"@types/vscode": "^1.85.0",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"@vscode/test-cli": "^0.0.4",
		"@vscode/test-electron": "^2.3.8",
		"chai": "4.1.0",
		"eslint": "^8.56.0",
		"jsdom": "^24.0.0",
		"ts-loader": "^9.5.1",
		"ts-mockito": "^2.6.1",
		"typescript": "^5.3.3",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"chart.js": "^4.4.1",
		"csv-parser": "^3.0.0",
		"dotenv": "^16.4.5",
		"dotenv-webpack": "^8.1.0",
		"uuid": "^9.0.1"
	}
}
